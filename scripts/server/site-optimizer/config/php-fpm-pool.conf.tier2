# Tier 2: Medium-Traffic Sites (5-7 sites per server)
# Use for: Standard business sites, moderate WooCommerce stores

[www]
pm = dynamic
pm.max_children = 10
pm.start_servers = 3
pm.min_spare_servers = 2
pm.max_spare_servers = 4
pm.max_requests = 500
pm.process_idle_timeout = 30s

; Monitoring endpoints
pm.status_path = /status
ping.path = /ping
ping.response = pong
