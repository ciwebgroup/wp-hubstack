# Tier 1: High-Traffic Sites (2-3 sites per server)
# Use for: Primary revenue sites, high-traffic WooCommerce stores

[www]
pm = dynamic
pm.max_children = 15
pm.start_servers = 4
pm.min_spare_servers = 2
pm.max_spare_servers = 6
pm.max_requests = 500
pm.process_idle_timeout = 30s

; Monitoring endpoints
pm.status_path = /status
ping.path = /ping
ping.response = pong
